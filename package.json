{
  "name": "recyclarr-wiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cross-env DOCUSAURUS_GENERATED_FILES_DIR_NAME=.docusaurus-current GITHUB_REF=refs/heads/master CURRENT_URL=http://localhost:3000 OTHER_URL=http://localhost:3001 docusaurus start --no-open",
    "start:next": "cross-env DOCUSAURUS_GENERATED_FILES_DIR_NAME=.docusaurus-next GITHUB_REF=refs/heads/next CURRENT_URL=http://localhost:3001 OTHER_URL=http://localhost:3000 docusaurus start --no-open --port 3001",
    "start:both": "concurrently -n current,next -c blue,magenta \"yarn start\" \"yarn start:next\"",
    "launch": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@algolia/client-search": "^5.2.1",
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@iconify/react": "^6.0.2",
    "@mdx-js/react": "^3",
    "@types/react": "^19.0.0",
    "clsx": "^2.0.0",
    "docusaurus-mdx-checker": "^3.0.0",
    "docusaurus-plugin-sass": "^0.2.6",
    "docusaurus-theme-github-codeblock": "^2.0.2",
    "prism-react-renderer": "^2.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-loadable": "^5.5.0",
    "sass": "^1.93.2",
    "search-insights": "^2",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
